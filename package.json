{
  "name": "heyhi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "android": "react-native run-android --verbose",
    "ios": "ENVFILE=.env react-native run-ios --verbose",
    "release": "react-native run-android --variant=release --verbose",
    "release:ios": "react-native run-ios --configuration Release --verbose",
    "reset": "cd android && ./gradlew clean && cd ..",
    "remove-cache": "watchman watch-del-all && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-*",
    "pretty": "prettier --write \"src/**/*.ts\"",
    "ts": "tsc -w",
    "lint": "eslint src --ext .ts,.tsx,.d.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "update-apollo": "apollo service:push"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.5",
    "@react-native-community/async-storage": "^1.10.1",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.2",
    "@react-navigation/material-bottom-tabs": "^5.2.2",
    "@react-navigation/native": "^5.3.0",
    "@react-navigation/stack": "^5.3.3",
    "@shopify/restyle": "^1.1.0",
    "@welldone-software/why-did-you-render": "^4.2.1",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-cache-persist": "0.1.1",
    "apollo-client": "2.6.9",
    "apollo-link": "1.2.14",
    "apollo-link-error": "1.1.13",
    "apollo-link-http": "1.5.17",
    "apollo-link-retry": "2.2.16",
    "aws-sdk": "^2.676.0",
    "axios": "^0.21.1",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "i18n-js": "^3.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "numeral": "^2.0.6",
    "react": "16.12.0",
    "react-native": "0.62.2",
    "react-native-bootsplash": "^2.2.4",
    "react-native-config": "^1.0.0",
    "react-native-confirmation-code-input": "git+https://github.com/SongTaehwan/react-native-confirmation-code-input.git#heyhi",
    "react-native-date-picker": "^2.7.10",
    "react-native-elements": "^2.0.0",
    "react-native-error-boundary": "^1.1.0",
    "react-native-exit-app": "^1.1.0",
    "react-native-fs": "^2.16.6",
    "react-native-geolocation-service": "^4.0.1",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-gifted-chat": "^0.16.3",
    "react-native-image-crop-picker": "^0.31.1",
    "react-native-image-resizer": "^1.2.2",
    "react-native-linear-gradient": "~2.5.6",
    "react-native-localize": "^1.4.0",
    "react-native-maps": "0.27.1",
    "react-native-material-dropdown": "^0.11.1",
    "react-native-modal": "^11.5.6",
    "react-native-paper": "^3.10.1",
    "react-native-permissions": "^2.1.4",
    "react-native-reanimated": "^1.8.0",
    "react-native-redash": "^14.2.3",
    "react-native-responsive-screen": "^1.4.1",
    "react-native-safe-area-context": "^1.0.0",
    "react-native-screens": "^2.7.0",
    "react-native-snap-carousel": "^3.9.1",
